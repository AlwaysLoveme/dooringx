{
  "name": "dooringx-ui",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev DID_YOU_KNOW=none max dev",
    "build": "max build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@umijs/preset-react": "1.x",
    "animate.css": "^4.1.1",
    "dooringx-core": "workspace:^"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^4.0.1",
    "@umijs/lint": "^4.0.74",
    "@umijs/max": "^4.0.74",
    "@umijs/plugins": "^4.0.74",
    "@umijs/test": "^3.5.4",
    "cross-env": "^7.0.3",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
